# 데이터베이스 설정 (AWS Aurora/RDS)
SPRING_DATASOURCE_URL=jdbc:mysql://your-aurora-endpoint:3306/your-database?serverTimezone=Asia/Seoul
SPRING_DATASOURCE_USERNAME=your-username
SPRING_DATASOURCE_PASSWORD=your-password

# JWT 설정 (특수문자가 포함된 경우 따옴표로 감싸세요)
# 예시: JWT_SECRET="2Jo!]8VlO)ohjFt5]9.h=n>}Knn!icdHTdT-7ra!:c.\"$oD#rh\"#*kO*#y}^}{ZE}"
JWT_SECRET="your-jwt-secret-key-here"

# 카카오 OAuth 설정
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
# 도메인 옵션들:
# 로컬 테스트: http://localhost/auth/callback/kakao
# AWS EC2: http://ec2-xx-xxx-xxx-xxx.ap-northeast-2.compute.amazonaws.com/auth/callback/kakao
# 커스텀 도메인: http://your-domain.com/auth/callback/kakao
KAKAO_REDIRECT_URI=http://localhost/auth/callback/kakao

# 네이버 OAuth 설정
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret
# 도메인 옵션들:
# 로컬 테스트: http://localhost/auth/callback/naver  
# AWS EC2: http://ec2-xx-xxx-xxx-xxx.ap-northeast-2.compute.amazonaws.com/auth/callback/naver
# 커스텀 도메인: http://your-domain.com/auth/callback/naver
NAVER_REDIRECT_URI=http://localhost/auth/callback/naver
