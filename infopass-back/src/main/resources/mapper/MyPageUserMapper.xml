<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="boot.infopass.mapper.MyPageUserMapperInter">
  <select id="findUserById" parameterType="int" resultType="boot.infopass.dto.UserDto">
    SELECT id, name, email, exp, level, created_at
    FROM user
    WHERE id = #{id}
  </select>
</mapper>
